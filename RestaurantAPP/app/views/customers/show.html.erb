<div class="row">
  <div class="col s12 center">
    <h2>Customer <%= @customer.name %></h2>
    <% @items.each do |item| %>
    <a class="black-text" href="/items/<%= item.id %>">
    <div class="card">
      <div class="card-content">
        <h4><%= item.name %> : $<%= item.price %></h4>
      </div>
    </div>
    </a>
    <% end %>
  </div>
</div>
